#pragma GCC system_header

#ifndef _LIBCXX_CSTRING
#define _LIBCXX_CSTRING

#include <__config>
#include <string.h>

_LIBCXX_BEGIN_NAMESPACE_STD

using ::memcmp;
using ::memcpy;
using ::memmove;
using ::memset;
using ::strcmp;
using ::strcpy;
using ::strlen;
using ::strncmp;
using ::strncpy;

_LIBCXX_END_NAMESPACE_STD

#endif // _LIBCXX_CSTRING